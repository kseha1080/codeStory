<h1>Welcome {{user.firstName}}</h1>
<h4>You Code</h4>
{{#if stories}}
<table class="striped">
  <thead>
    <tr>
      <th>Title</th>
      
      <th>Status</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each stories}}
    <tr>
      <td><a href="/stories/show/{{id}}">{{title}}</a></td>
      <td><span class="dash-status">{{status}}</span></td>
      <td><a href="/stories/edit/{{id}}" class="btn left-align"><i class="fa fa-pencil"></i> Edit</a>

      <form action="/stories/{{id}}?_method=DELETE" method="post" id="delete-form">
      <input type="hidden" name="_method" value="DELETE">
      <button type="submit" class="btn red"><i class="fa fa-remove"></i> Delete</button>
      </form>
      </td>
    </tr>
    {{/each}}
  </tbody>

</table>
{{else}}
  <p>What are you waiting make some code</p>
{{/if}}